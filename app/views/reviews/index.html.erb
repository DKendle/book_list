<h1><%= @current_user.username%>'s Reviews</h1>
<%= link_to "Create a New Review?", new_user_review_path( @current_user)%>
<%= link_to "Back to Dashboard", user_path( @current_user) %>

<hr class="rounded">
<ul>


<% @reviews.each do |review| %>
    <li>
    <%= link_to review.title, user_review_path( @current_user, review)%>  
    </li>
<% end %>
</ul>